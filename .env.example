# Clerk Authentication Keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_***
CLERK_SECRET_KEY=sk_test_***

# Clerk URLs
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/onboarding

# JWT Verification Key (for local development)
# Get this from Clerk Dashboard -> JWT Keys -> Show JWKS -> Copy Public Key
CLERK_JWT_KEY=-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----

# Clerk Webhook Secret
# Get this from Clerk Dashboard -> Webhooks -> Add Endpoint -> Copy Secret
CLERK_WEBHOOK_SECRET=whsec_***

# Supabase Storage (for image uploads)
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe Payment Processing
# Get these from Stripe Dashboard -> Developers -> API keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_***
STRIPE_SECRET_KEY=sk_test_***

# Stripe Webhook Secret
# Get this from Stripe Dashboard -> Developers -> Webhooks -> Add Endpoint -> Copy Secret
STRIPE_WEBHOOK_SECRET=whsec_***

# Stripe Price IDs (for products)
# Get these from Stripe Dashboard -> Products -> Pricing
STRIPE_PRICE_ID_MONTHLY=price_***
STRIPE_PRICE_ID_YEARLY=price_***

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# SerpAPI (for SERP analysis)
# Get your API key from https://serpapi.com/
SERPAPI_API_KEY=your-serpapi-key-here
SERPAPI_BASE_URL=https://serpapi.com
SERPAPI_TIMEOUT=30000
SERPAPI_MAX_RETRIES=3
SERPAPI_RETRY_DELAY=1000
SERPAPI_ENABLE_CACHE=true
SERPAPI_CACHE_TTL=3600
SERPAPI_DEFAULT_DOMAIN=google.com
SERPAPI_DEFAULT_LANGUAGE=en
SERPAPI_DEFAULT_COUNTRY=us
SERPAPI_DEFAULT_DEVICE=desktop
SERPAPI_DEFAULT_SAFE=off
SERPAPI_DEFAULT_NUM=10

# Upstash Redis (for rate limiting and usage tracking)
# Get these from https://upstash.com/
# Create a new Redis database and copy the REST API credentials
UPSTASH_REDIS_REST_URL=https://your-redis-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token

# PostHog Analytics
# Get these from your PostHog project settings: Project -> Project Settings -> Keys
NEXT_PUBLIC_POSTHOG_KEY=phc_***
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Moz API (for Domain Authority and link metrics)
# Get your access ID and secret from https://moz.com/products/api
MOZ_API_ACCESS_ID=your-moz-access-id-here
MOZ_API_SECRET_KEY=your-moz-secret-key-here
MOZ_API_BASE_URL=https://lsapi.seomoz.com/anchor/v1
MOZ_API_TIMEOUT=30000
MOZ_CACHE_TTL=2592000

# DataForSEO (for Rank Tracking)
# Get your credentials from https://dataforseo.com/
DATAFORSEO_USERNAME=your-dataforseo-username
DATAFORSEO_PASSWORD=your-dataforseo-password
DATAFORSEO_API_BASE_URL=https://api.dataforseo.com/v3
DATAFORSEO_TIMEOUT=60000
DATAFORSEO_BATCH_SIZE=50
DATAFORSEO_BATCH_DELAY=1000

# Rank Tracking Cron Secret
# Used to authenticate cron job requests to the rank tracking worker
# Generate a strong random string for production
RANK_TRACKING_CRON_SECRET=your-rank-tracking-cron-secret-here
